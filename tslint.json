{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "prefer-for-of": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "arrow-parens": false
  }
}
